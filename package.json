{
  "name": "master_mind",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build": "babel src -d lib",
    "bundle-android": "react-native bundle --entry-file index.js  --platform android --dev false --bundle-output ./android/app/src/main/assets/index.android.bundle",
    "test": "jest",
    "lint": "eslint . --quiet",
    "incrementBuild": "react-native-version -b",
    "npm:clean": "rm -rf node_modules && npm i",
    "android:clean": "cd android && ./gradlew clean && cd ../",
    "android:run": "node_modules/.bin/react-native run-android",
    "android:runRelease": "node_modules/.bin/react-native run-android --variant=release",
    "android:assembleRelease": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ../",
    "bundle:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "ios:clean": "cd ios && rm -rf build && rm -rf Pods && rm Podfile.lock && pod install && cd ../",
    "postinstall": "npx jetify",
    "clean": "npm run npm:clean && npm run android:clean && npm run ios:clean"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.4",
    "@react-native-community/google-signin": "^5.0.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/viewpager": "^4.1.6",
    "@react-native-firebase/admob": "^10.7.0",
    "@react-native-firebase/app": "^10.7.0",
    "@react-native-firebase/auth": "^10.7.0",
    "@react-native-firebase/database": "^10.7.0",
    "@react-native-firebase/firestore": "^10.7.0",
    "@react-native-firebase/messaging": "^10.7.0",
    "@react-native-firebase/storage": "^10.7.0",
    "@react-navigation/native": "^5.7.6",
    "@react-navigation/stack": "^5.9.3",
    "getsocial-react-native-sdk": "^7.1.1",
    "moment": "2.29.1",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-contacts": "^5.2.7",
    "react-native-emoji-board": "^1.2.1",
    "react-native-fast-image": "^8.3.2",
    "react-native-fbsdk": "^2.0.0",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-gifted-chat": "^0.16.3",
    "react-native-image-picker": "^2.3.4",
    "react-native-iphone-x-helper": "^1.3.0",
    "react-native-keyboard-avoiding-scroll-view": "^1.0.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^11.5.6",
    "react-native-modal-dropdown": "^0.7.0",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.11.0",
    "react-native-simple-radio-button": "^2.7.4",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^6.2.1",
    "react-native-svg-charts": "^5.4.0",
    "react-native-switch": "^2.0.0",
    "react-native-vector-icons": "^7.0.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/runtime": "7.11.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "25.5.1",
    "eslint": "^7.6.0",
    "jest": "25.5.4",
    "metro-react-native-babel-preset": "0.59.0",
